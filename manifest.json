{
  "content_scripts": [
    {
      "js": ["script.js"],
      "matches": ["https://www.youtube.com/*"]
    }
  ],
  "manifest_version": 3,
  "name": "Youtube Basic",
  "description": "Testing out simple youtube extension",
  "version": "1.1.2",
  "action": {
    "default_popup": "index.html",
    "default_icon": "/icons/extension_icon.png"
  }
}