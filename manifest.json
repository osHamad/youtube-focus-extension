{

  "manifest_version": 3,
  "name": "Youtube Basic",
  "description": "Testing out simple youtube extension",
  "version": "0.2.1",

  "action": {
    "default_popup": "index.html",
    "default_icon": "/icons/extension_icon.png"
  },

  "content_scripts": [
    {
      "all_frames": true,
      "run_at": "document_start",
      "js": ["scripts/home.js"],
      "matches": ["https://www.youtube.com/*"]
    }
  ]
  
}